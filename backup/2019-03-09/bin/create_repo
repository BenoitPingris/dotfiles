blih -u benoit.pingris@epitech.eu repository create $MY_REPO
blih -u benoit.pingris@epitech.eu repository setacl $MY_REPO ramassage-tek r
git clone git@git.epitech.eu:/benoit.pingris@epitech.eu/$MY_REPO
